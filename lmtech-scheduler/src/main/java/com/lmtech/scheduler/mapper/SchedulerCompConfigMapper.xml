        <result column="comp_type" property="compType"></result>
        <result column="bean_name" property="beanName"></result>
        <result column="component_class" property="componentClass"></result>
        <result column="invoke_method" property="invokeMethod"></result>
        <result column="service_host" property="serviceHost"></result>
        <result column="method_url" property="methodUrl"></result>
        <result column="method_type" property="methodType"></result>
        <result column="description" property="description"></result>
    </resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id,create_date, create_user, create_user_name, update_date, update_user, update_user_name, data_version, is_delete
    </sql>

</mapper>